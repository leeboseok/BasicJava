package exam;

import java.util.Scanner;

public class Sample2 {
	public static void main(String[] args) {
		
		Student s=new Student("이보석");
		
	}
}

class Student{
	int name;
	int mathGrade;
	int englishGrade;
	int koreanGrade;
	
	Student(String a){
		name=a;
		System.out.println("이름: "+"name"+"\t"+"수학: "+mathGrade+"\t"+"영어: "+englishGrade+"\t"+"국어: "+koreanGrade);
		
	}
}